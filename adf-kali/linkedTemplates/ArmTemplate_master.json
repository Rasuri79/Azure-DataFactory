{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-kali"
		},
		"Compare1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Compare1'"
		},
		"Compare2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Compare2'"
		},
		"Kalilake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Kalilake'"
		},
		"Compare1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lalithaadfcourse.dfs.core.windows.net/"
		},
		"Compare2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lalithaadfcourse.dfs.core.windows.net/"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"Kalilake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lalithaadfcourse.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-kali_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Compare1_accountKey": {
						"value": "[parameters('Compare1_accountKey')]"
					},
					"Compare2_accountKey": {
						"value": "[parameters('Compare2_accountKey')]"
					},
					"Kalilake_accountKey": {
						"value": "[parameters('Kalilake_accountKey')]"
					},
					"Compare1_properties_typeProperties_url": {
						"value": "[parameters('Compare1_properties_typeProperties_url')]"
					},
					"Compare2_properties_typeProperties_url": {
						"value": "[parameters('Compare2_properties_typeProperties_url')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"Kalilake_properties_typeProperties_url": {
						"value": "[parameters('Kalilake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-kali_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-kali_ArmTemplate_0')]"
			]
		}
	]
}